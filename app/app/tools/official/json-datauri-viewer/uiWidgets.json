[
  [
    {
      "id": "json-input",
      "type": "TextareaInput",
      "title": "JSON Input",
      "mode": "input",
      "props": {
        "placeholder": "Paste JSON containing Base64 Data URIs...",
        "rows": 10,
        "defaultValue": "{\n  \"name\": \"Sample Data\",\n  \"image\": \"data:image/svg+xml;base64,\",\n  \"audio\": \"data:audio/vnd.dlna.adts;base64,\",\n  \"video\": \"data:video/mp4;base64,\",\n  \"nested\": {\n    \"nested\": {\n      \"nested\": {\n        \"image\": \"data:image/svg+xml;base64,\"\n      }\n    }\n  }\n}",
        "highlight": "highlight:json"
      }
    }
  ],  
  [
    {
      "id": "indent-size",
      "type": "NumberInput",
      "title": "Indent",
      "mode": "input",
      "props": {
        "prefixLabel": "Spaces",
        "min": 1,
        "max": 8,
        "step": 1,
        "defaultValue": 2
      }
    },
    {
      "id": "show-path",
      "type": "ToggleInput",
      "title": "Show JSON Path",
      "mode": "input",
      "props": {
        "defaultValue": true,
        "description": "Display the JSON path for each Data URI field"
      }
    }
  ],
  [
    {
      "id": "media-preview",
      "type": "LabelInput",
      "title": "Data URI Media Preview",
      "mode": "output",
      "props": {
        "content": "<div class='text-xs text-muted-foreground italic'>Paste JSON to preview embedded Data URIs...</div>",
        "autoHeight": true
      }
    }
  ]
]
